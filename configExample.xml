<?xml version="1.0" encoding="UTF-8"?>
<cfg>
    <server>
        <domains>
            <domain env='PROD'>
                <protocol>http</protocol>
                <fqdn>contoso.com</fqdn>
            </domain>
            <domain env='DEV'>
                <protocol>http</protocol>
                <fqdn>dev.contoso.com</fqdn>
            </domain>
        </domains>
        <path>path</path>
        <token>token</token>
        <public_key>public_key</public_key>
    </server>
    <message>
        <from>admin@contosom.com</from>
        <to>
            <email>1@contosom.com</email>
            <email>2@contosom.com</email>
            <email>n@contosom.com</email>
        </to>
    </message>
    <smtp>
        <server>outlook.office365.com</server>
        <port>587</port>
        <username>admin@contosom.com</username>
        <password>password</password>
    </smtp>
    <dashing>
        <url>http://dashing.contoso.com</url>
        <token>token</token>
    </dashing>
    <ad>
        <ou>
            <staff>Staff</staff>
            <students>Students</students>
        </ou>
    </ad>
    <patch>
        <folder>C:\folder</folder>
        <filedataformat>dd.MM.yyyy</filedataformat>
    </patch>
</cfg>
